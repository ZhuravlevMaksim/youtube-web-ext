{
  "manifest_version": 2,
  "name": "youtube-web-ext",
  "version": "1.0",
  "description": "Get audio from youtube video",
  "icons": {
    "48": "icons/border-48.png"
  },
  "applications": {
    "gecko": {
      "id": "maks723@yandex.ru",
      "strict_min_version": "55.0"
    }
  },
  "permissions": [
    "activeTab",
    "contextMenus",
    "nativeMessaging"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/border-48.png"
  }
}